            <article>
              
                    <figure class="right add-border" style="max-width:450px; clear:right;">
                      
                        <a href="#Image-232" title="View larger image" onClick="document.getElementById('modal-232').style.display = 'block';">
                        <img src="/CFFileServlet/_cf_image/_cfimg-7867119659878586768.PNG" alt="figure" id="Image-232" />

